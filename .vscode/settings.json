{
  "files.exclude": {
    ".git/": true,
    "node_modules/": true,
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
  },
  "editor.tabCompletion": "on",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "cSpell.words": [
    "agingdeveloper",
    "astro",
    "astrojs",
    "frontmatter",
    "pinterest",
    "tailwindcss",
    "uncategorized",
    "vitest"
  ],
}